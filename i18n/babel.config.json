{
	"presets": ["@babel/preset-typescript", "@babel/preset-react"],
	"plugins": [
	  [
	    "i18next-extract",
	    {
	      "locales": ["en", "ja", "zh", "ko"],
	      "outputPath": "./i18n/locales/{{locale}}.json",
	      "discardOldKeys": false,
	      "keySeparator": null
	    }
	  ]
	]
        }